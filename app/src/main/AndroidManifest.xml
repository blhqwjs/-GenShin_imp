<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.genshin_imp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CharacterDetailsActivity"></activity>
        <activity android:name=".CharacterListActivity"></activity>
        <activity android:name=".Dao_Controller.CharacterManager" />
        <activity android:name=".AddCharacterActivity" />
        <activity android:name=".monster.AddMonsterActivity" />
        <activity android:name=".monster.ManageMonsterActivity" />
        <activity android:name=".monster.ViewEditMonsterActivity" />
        <activity
            android:name=".EditPropertiesActivity"
            android:label="Edit Properties Activity">
        </activity>
        <activity android:name=".Videos.VideosActivity"></activity>
        <activity android:name=".WebView.WebViewActivity"></activity>
        <service android:name=".Videos.VideoPlayerService" />
        <!--视频播放功能权限-->
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
        <!--网络权限-->

</manifest>